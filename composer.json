{
    "name": "wisdomanthoni/signature",
    "description": "A Laravel Package providing SSO and OAUTH",
    "keywords": [
        "sso",
        "client",
        "server",
        "laravel",
        "socialite",
        "session",
        "oauth2"
    ],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "league/oauth2-client": "^2.3",
        "laravel/socialite": "^3.1",
        "jasny/sso": "^0.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Staybusy\\Signature\\": "src",
            "Staybusy\\Signature\\Test\\": "tests/"
        }
    },  
    "extra": {
        "laravel": {
            "providers": [
                "Staybusy\\Signature\\ServiceProvider"
            ],
            "aliases": {
                "Signature": "Staybusy\\Signature\\SignatureFacade"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Wisdom Anthony",
            "email": "wizitendo10@gmail.com"
        }
    ]
}
